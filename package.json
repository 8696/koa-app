{
  "name": "koa-node-server",
  "version": "1.0.0",
  "description": "这个项目是一个使用 Koa 框架构建的 Node.js 应用程序。它利用了多种现代开发工具和实践，以确保可维护性、可扩展性和开发人员的生产力",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --ext ts --exec 'cross-env NODE_ENV=development FEATURE_ENV=development ts-node ./src/app.ts'",
    "build": "rimraf build && yarn eslint && tsc",
    "eslint": "eslint ./src --fix",
    "sync.db": "cross-env FEATURE_ENV=production ts-node ./src/script/sync.db.ts",
    "pm2:dev": "yarn build && yarn pm2:delete && pm2 start pm2.config.js --env development",
    "pm2:test": "yarn build && yarn pm2:delete && pm2 start pm2.config.js --env test",
    "pm2:production": "yarn build && yarn pm2:delete && pm2 start pm2.config.js --env production",
    "pm2:stop": "pm2 stop pm2.config.js",
    "pm2:restart": "pm2 restart pm2.config.js",
    "pm2:delete": "pm2 delete pm2.config.js",
    "ncu:check": "ncu --target minor",
    "ncu:update": "ncu --target minor -u"
  },
  "keywords": [],
  "author": "long <<y201899@gmail.com>>",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "5.0.0",
    "@types/blueimp-md5": "2.18.2",
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "@types/koa-static": "4.0.4",
    "@types/koa__cors": "3.3.1",
    "@types/lodash": "4.17.6",
    "@types/sequelize": "4.28.20",
    "@types/validator": "13.12.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "address": "1.2.2",
    "axios": "0.28.1",
    "blueimp-md5": "2.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "ioredis": "^5.4.1",
    "koa": "2.15.3",
    "koa-router": "9.4.0",
    "koa-static": "5.0.0",
    "lodash": "4.17.21",
    "log4js": "6.9.1",
    "mysql2": "3.10.1",
    "nodemon": "2.0.22",
    "reflect-metadata": "^0.2.2",
    "rimraf": "5.0.7",
    "routing-controllers": "^0.10.4",
    "sequelize": "6.37.3",
    "ts-node": "10.9.2",
    "typescript": "4.9.5",
    "validator": "13.12.0"
  },
  "homepage": "https://github.com/8696/koa-app/#readme",
  "bugs": {
    "url": "https://github.com/8696/koa-app/issues"
  }
}
